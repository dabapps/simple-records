{
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "react",
    "target": "es5",
    "lib": [
      "es2015.iterable",
      "es2015.promise",
      "dom",
      "es5",
      "scripthost"
    ],
    "typeRoots": [
      "node_modules/@types/",
      "types/"
    ]
  },
  "exclude": [
    "./dist/",
    "./node_modules/"
  ]
}
